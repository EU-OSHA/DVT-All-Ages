<?xml version="1.0" encoding="UTF-8"?>
<plugin title="all-ages" loader="DEFAULT">
    <overlays>
        <overlay id="dataaccess" resourcebundle="api/messages/messages" loadatstart="false"></overlay>
    </overlays>
    <lifecycle-listener class="pt.webdetails.cpk.CpkLifecycleListener"/>
    <static-paths>
        <static-path url="/all-ages/static" localFolder="static"/>
        <static-path url="/all-ages/resources" localFolder="resources"/>
        <static-path url="/all-ages/seo" localFolder="static/custom/seo/"/>
    </static-paths>
    <content-types>
        <content-type>
            <title>European Agency for Safety &amp; Health at Work - Data Visualisation Tool - All Ages</title>
            <description>All Ages by Zylk.net</description>
            <company name="Zylk.net" url="" logo="@PLUGIN_COMPANY_LOGO@"></company>
            <icon-url>api/repos/all-ages/static/custom/img/favicon.ico</icon-url>
        </content-type>
    </content-types>
    <content-generator
        id="all-ages"
        title="All Ages by Zylk.net"
        type="all-ages"
        class="pt.webdetails.cpk.CpkContentGenerator"/>
    <!-- Menu entry -->
    <menu-items>

        <!--<main-menu-item id="all-ages_main" anchor="tools-submenu" label="Older Workers Pilot" command="content/all-ages/" type="MENU_ITEM" how="LAST_CHILD"/>-->
    </menu-items>

    <external-resources>
        <!--<file context="all-ages">api/repos/all-ages/static/all-ages-require-js-cfg.js</file>-->
    </external-resources>
</plugin>
